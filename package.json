    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.0.0",
    "eslint": "^9.12.0",
    "eslint-config-next": "14.2.3",
