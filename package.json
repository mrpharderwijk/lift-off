{
  "name": "@lift-off/workspace",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "loui:build": "nx build lift-off-ui",
    "loui:storybook": "nx run lift-off-ui:storybook --port=6006",
    "loui:deploy": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --package lift-off-ui",
    "loui:test": "nx test lift-off-ui",
    "loui:test:coverage": "nx test lift-off-ui --coverage",
    "loui:deploy:patch": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version patch --package lift-off-ui",
    "loui:deploy:minor": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version minor --package lift-off-ui",
    "loui:deploy:major": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version major --package lift-off-ui",
    "eslint-config:build": "nx build eslint-config",
    "eslint-config:deploy": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --package eslint-config",
    "eslint-config:deploy:patch": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version patch --package eslint-config",
    "eslint-config:deploy:minor": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version minor --package eslint-config",
    "eslint-config:deploy:major": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version major --package eslint-config",
    "deploy:all": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --package all",
    "deploy:all:patch": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version patch --package all",
    "deploy:all:minor": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version minor --package all",
    "deploy:all:major": "ts-node --project scripts/tsconfig.json scripts/deploy-package.ts --version major --package all",
    "test-app:dev": "nx dev test-app",
    "test-app:build": "nx build test-app",
    "test-app:start": "nx start test-app"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.tsx": [
      "eslint --fix",
      "prettier --write"
    ]
  },
