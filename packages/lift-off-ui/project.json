{
  "name": "lift-off-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lift-off-ui/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project lift-off-ui --web",
  "targets": {
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "storybook dev -p {args.port}",
        "cwd": "packages/lift-off-ui"
      }
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "storybook build",
        "cwd": "packages/lift-off-ui"
      }
    }
  }
}
